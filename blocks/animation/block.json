{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "tumult-hype-animations/animation",
  "version": "1.0.0",
  "title": "Tumult Hype Animation",
  "category": "embed",
  "icon": "format-video",
  "description": "Embed a Tumult Hype animation",
  "supports": {
    "html": false,
    "align": ["wide", "full"]
  },
  "textdomain": "tumult-hype-animations",
  "editorScript": "tumult-hype-animations-editor",
  "editorStyle": "tumult-hype-animations-editor",
  "attributes": {
    "animationId": {
      "type": "number",
      "default": 0
    },
    "width": {
      "type": "string",
      "default": ""
    },
    "height": {
      "type": "string",
      "default": ""
    },
    "embedMode": {
      "type": "string",
      "default": "div"
    },
    "embedMode": {
      "type": "string",
      "default": "div"
    },
    "originalWidth": {
      "type": "string",
      "default": ""
    },
    "originalHeight": {
      "type": "string",
      "default": ""
    }
  },
  "example": {
    "attributes": {
      "animationId": 1,
      "isResponsive": true
    }
  }
}
